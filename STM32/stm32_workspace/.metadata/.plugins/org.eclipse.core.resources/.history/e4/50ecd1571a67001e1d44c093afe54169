#include "main.h"
#include <stdbool.h>
#include <stdint.h>
#include <stdlib.h>
#include "esp_12f.h"
#include <string.h>

char *g_esp12f_str;

char* esp12f_send_command(esp12f_commands_e cmd, uint16_t size)
{
    bool param = false;
    char *g_esp12f_str = 0;
    switch (cmd)
    {
        case ESP12F_STARTUP:
        break;
        case ESP12F_RESTART:
        break;
        case ESP12F_VERSION_INFO:
            param = true;
            g_esp12f_str = "+GMR";
        break;
        case ESP12F_ENTER_DEEP_SLEEP:
            param = true;
            g_esp12f_str = "+GSL";
        break;
        case ESP12F_ECHO_ENABLE:
            g_esp12f_str = "+ATE1";
        break;
        case ESP12F_ECHO_DISABLE:
            g_esp12f_str = "+ATE0";
        break;
        case ESP12F_RESET:
        break;
        case ESP12F_UART_CONFIG:
        break;
        case ESP12F_UART_CURRENT_CONFIG:
        break;
        case ESP12F_UART_DEFAULT_CONFIG:
        break;
        case ESP12F_SLEEP:
        break;
        case ESP12F_SET_MAX_RF_POWER:
        break;
        case ESP12F_SET_MAX_RF_POWER_VDD33:
        break;
    }
}
