#include "main.h"
#include <stdbool.h>
#include <stdint.h>
#include <stdlib.h>
#include "esp_12f.h"
#include <string.h>


void esp12f_send_command(esp12f_commands_e cmd, uint16_t size)
{
    bool param = false;
    char *str;
    switch (cmd)
    {
        case ESP12F_STARTUP:

        break;
        case ESP12F_RESTART:

        break;
        case ESP12F_VERSION_INFO:
            str = "+GMR";
        break;
        case ESP12F_ENTER_DEEP_SLEEP:
            str = "+GSL";
        break;
        case ESP12F_ECHO_OR_NOT:
            str = "+ATE0";
            str = "+ATE1";
        break;
        case ESP12F_RESET:
        break;
        case ESP12F_UART_CONFIG:
        break;
        case ESP12F_UART_CURRENT_CONFIG:
        break;
        case ESP12F_UART_DEFAULT_CONFIG:
        break;
        case ESP12F_SLEEP:
        break;
        case ESP12F_SET_MAX_RF_POWER:
        break;
        case ESP12F_SET_MAX_RF_POWER_VDD33:
        break;
    }
}
