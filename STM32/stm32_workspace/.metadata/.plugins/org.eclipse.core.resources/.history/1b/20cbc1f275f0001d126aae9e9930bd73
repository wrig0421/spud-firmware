// SRW
#include <stdbool.h>
#include "stm32l4xx_hal.h"
#include "cmsis_os.h"
#include "pkt.h"
#include "task_uart.h"


// rx from computer
void task_uart_rx(void *argument)
{
    while (1)
    {
        pkt_parse_rx(pkt_dequeue(pkt_dequeue_from_free()));
    }
}


// tx to computer
void task_uart_tx(void *argument)
{
    while (1)
    {
        pkt_parse_tx(pkt_dequeue(pkt_dequeue_from_free()));
    }
}
